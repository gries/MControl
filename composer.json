{
    "name":         "gries/mcontrol",
    "description":  "PHP Library to controll a running Minecraft-Server",
    "license":      "MIT",
    "keywords":     ["Minecraft", "Admin"],
    "type":         "library",
    "authors":      [
        {
            "name":      "Christoph Rosse",
            "email":     "christoph@rosse.at"
        }
    ],
    "require": {
        "monolog/monolog": "~1.6",
        "doctrine/orm": "~2.4",
        "gries/php-source-query-class": "dev-master",
        "symfony/dom-crawler": "~2.4"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev"
    },
    "suggest": {
        "ext-imagick": "To use the ImageToStructureConverter the Imagick extension for php is required."
    },
    "autoload": {
        "psr-0": {"gries\\MControl": "src/"}
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    }
}
